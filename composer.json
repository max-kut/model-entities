{
    "name": "maxkut/model-entities",
    "description": "wrapper for json objects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Maksim Kutishchev",
            "email": "max@amopro.ru"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/contracts": "5.5.*|5.6.*|5.7.*|5.8.*|6.*|7.*",
        "nesbot/carbon": "1.*|2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Entities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
